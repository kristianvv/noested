@{
}

@model Noested.Models.ViewModels.UserListViewModel

<h2>Registered Users</h2>

<table>
    <tr>
        <th>Email</th>
        <!-- Flere kolonner etter behov -->
    </tr>
    @foreach (var user in Model.Users)
    {
        <tr>
            <td>@user.Email</td>
            <!-- Flere celler etter behov -->
        </tr>
    }
</table>
